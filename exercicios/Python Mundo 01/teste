
for _ in range(1, 10 + 1):
    num = int(input(' Digite um número : '))
    cont=0
    
    for n1 in range(1,num + 1):
        print (f'{n1} ', end= ' ')
        if num % n1 == 0:
            cont += 1         
    if cont > 2 or cont <= 1:
        print (f'o número {num} foi divisível {cont} vezes \n Não é PRIMO ')
    else :
        print (f'o número {num} foi divisível {cont} vezes \n É PRIMO')
    
    