

for pessoa in range (1, 5 + 1):
    peso= float(input('Digite um peso'))
    if pessoa == 1:
        maiorPeso=peso
        menorPeso=peso
    elif peso > maiorPeso:
        
        maiorPeso= peso 
    
    elif peso < menorPeso:
        
        menorPeso= peso
    
print (f'maior {maiorPeso}')

print (f' menor {menorPeso}')